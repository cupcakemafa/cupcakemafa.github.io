        <div id='body' class='row'>
            <!-- Start of Main -->
            <div id='main-area' class='col col-xs-12 col-sm-8'>
                <b:section class='main' id='main' showaddelement='no'>
                    <b:widget id='Blog1' locked='true' title='ブログの投稿' type='Blog'>
                        <b:includable id='main'>
                            <b:loop values='data:posts' var='post'>
                                <article class='post panel panel-default' itemprop='blogPost' itemscope='itemscope' itemtype='http://schema.org/BlogPosting'>
                                    <div id='post-url' class='param'><data:post.url/></div>

                                    <!-- Start meta data -->
                                    <b:if cond='data:blog.pageType == &quot;item&quot; or data:blog.pageType == &quot;static_page&quot;'>
                                        <b:if cond='data:post.canonicalUrl'>
                                            <div class='meta-tag param'>
                                                <div class='attr' data-key='itemprop'>url</div>
                                                <div class='attr' data-key='content'><data:post.canonicalUrl/></div>
                                            </div>
                                            <div class='param meta-tag'>
                                                <div class='attr' data-key='name'>og:url</div>
                                                <div class='attr' data-key='content'><data:blog.canonicalUrl/></div>
                                            </div>
                                        </b:if>
                                        <b:if cond='data:post.firstImageUrl'>
                                            <div class='meta-tag param'>
                                                <div class='attr' data-key='itemprop'>image</div>
                                                <div class='attr' data-key='content'><data:post.firstImageUrl/></div>
                                            </div>
                                            <div class='param meta-tag'>
                                                <div class='attr' data-key='name'>twitter:card</div>
                                                <div class='attr' data-key='content'>summary_large_image</div>
                                            </div>
                                            <div class='param meta-tag'>
                                                <div class='attr' data-key='name'>twitter:image:src</div>
                                                <div class='attr' data-key='content'><data:post.firstImageUrl/></div>
                                            </div>
                                            <div class='param meta-tag'>
                                                <div class='attr' data-key='name'>og:image</div>
                                                <div class='attr' data-key='content'><data:post.firstImageUrl/></div>
                                            </div>
                                        </b:if>
                                        <b:if cond='data:post.title'>
                                            <div class='param meta-tag'>
                                                <div class='attr' data-key='name'>twitter:title</div>
                                                <div class='attr' data-key='content'><data:post.title/></div>
                                            </div>
                                            <div class='param meta-tag'>
                                                <div class='attr' data-key='name'>og:title</div>
                                                <div class='attr' data-key='content'><data:post.title/></div>
                                            </div>
                                        </b:if>
                                        <b:if cond='data:blog.metaDescription'>
                                            <div class='param meta-tag'>
                                                <div class='attr' data-key='name'>twitter:description</div>
                                                <div class='attr' data-key='content'><data:blog.metaDescription/></div>
                                            </div>
                                        <b:else/>
                                            <div class='param meta-tag'>
                                                <div class='attr' data-key='name'>twitter:description</div>
                                                <div class='attr' data-key='content'><data:post.snippet/></div>
                                            </div>
                                        </b:if>
                                        <b:if cond='data:post.sharePostUrl'>
                                            <div class='param meta-tag'>
                                                <div class='attr' data-key='name'>twitter:url</div>
                                                <div class='attr' data-key='content'><data:post.sharePostUrl/></div>
                                            </div>
                                        </b:if>
                                        <b:if cond='data:post.pageName'>
                                            <div class='param meta-tag'>
                                                <div class='attr' data-key='name'>og:title</div>
                                                <div class='attr' data-key='content'><data:blog.pageName/></div>
                                            </div>
                                        </b:if>
                                        <b:if cond='data:blog.metaDescription'>
                                            <div class='param meta-tag'>
                                                <div class='attr' data-key='name'>og:description</div>
                                                <div class='attr' data-key='content'><data:blog.metaDescription/></div>
                                            </div>
                                        <b:elseif cond='data:blog.snippet'/>
                                            <div class='param meta-tag'>
                                                <div class='attr' data-key='name'>og:description</div>
                                                <div class='attr' data-key='content'><data:post.snippet/></div>
                                            </div>
                                        </b:if>
                                        <b:if cond='data:blog.timestampISO8601'>
                                            <div class='param meta-tag'>
                                                <div class='attr' data-key='name'>article:published_time</div>
                                                <div class='attr' data-key='content'><data:post.timestampISO8601/></div>
                                            </div>
                                        </b:if>
                                        <b:if cond='data:post.labels'>
                                            <b:loop values='data:post.labels' var='label'>
                                                <div class='param meta-tag'>
                                                    <div class='attr' data-key='name'>article:tag</div>
                                                    <div class='attr' data-key='content'><data:label.name/></div>
                                                </div>
                                            </b:loop>
                                        </b:if>
                                        <div class='param meta-tag'>
                                            <div class='attr' data-key='name'>article:section</div>
                                            <div class='attr' data-key='content'>Art, Culture, Lifestyle</div>
                                        </div>
                                    </b:if>
                                    <!-- End meta data -->

                                    <!-- Start post data -->
                                    <!-- for top -->
                                    <div class='panel-heading text-center'>
                                        <b:if cond='data:post.firstImageUrl'>
                                            <div class='text-center'>
                                                <img expr:src='data:post.firstImageUrl' expr:alt='data:post.title' class='thumb img-circle'/>
                                            </div>
                                        </b:if>
                                        <b:if cond='data:post.dateHeader'>
                                            <h5>
                                                <i class='fa fa-clock-o'/> 
                                                <data:post.dateHeader/>
                                            </h5>
                                        </b:if>
                                        <b:if cond='data:post.labels'>
                                            <nav>
                                                <h5>
                                                    <ul class='pager'>
                                                    <b:loop index = 'i' values = 'data:post.labels' var = 'label'>
                                                        <li>
                                                            <a class='button button-small button-stable activated' expr:href='data:label.url'>
                                                                <data:label.name/>
                                                            </a>
                                                        </li>
                                                    </b:loop>
                                                    </ul>
                                                </h5>
                                            </nav>
                                        </b:if>
                                        <h2 class='post-title panel-title'>
                                            <a expr:href='data:post.url' expr:title='data:post.title'>
                                                <data:post.title/>
                                            </a>
                                        </h2>
                                    </div>
                                    <div expr:id='&quot;post-&quot; + data:post.id' class='panel-body'>
                                        <b:if cond='data:blog.pageType == &quot;item&quot; or data:blog.pageType == &quot;static_page&quot;'>
                                            <data:post.body/>
                                        <b:else/>
                                            <div class='post-summary hidden'>
                                                <div id='id'><data:post.id/></div>
                                                <div id='title'><data:post.title/></div>
                                                <div id='url'><data:post.url/></div>
                                                <div id='content'>
                                                    <data:post.body/>
                                                </div>
                                            </div>
                                        </b:if>
                                            
                                        <b:if cond='data:blog.pageType == &quot;item&quot; or data:blog.pageType == &quot;static_page&quot;'>

                                            <b:if cond='data:post.showLocation'>
                                                <b:if cond='data:post.location'>
                                                    <div class='item post-location'>
                                                        <p>
                                                            <h3>
                                                                <i class='fa fa-map-marker'/> 地図リンク<br/>
                                                            </h3>
                                                            <big>
                                                                <strong>
                                                                    ADDRESS
                                                                </strong>
                                                            </big>
                                                            <hr class="embed quater-width" />
                                                            <h4>
                                                                <data:post.location.name/><br />
                                                                <big>
                                                                    <a class='map-link button' expr:href='data:post.location.mapsUrl' target='_blank'>
                                                                        <i class='fa fa-location-arrow'/> Google Map でみる
                                                                    </a>
                                                                </big>
                                                            </h4>
                                                        </p>
                                                    </div>
                                                </b:if>
                                            </b:if>

                                            <b:if cond='data:post.labels'>
                                                <nav>
                                                    <h3 class='text-center'><i class='fa fa-tags'/> Tags</h3>
                                                    <h5>
                                                        <ul class='pager'>
                                                        <b:loop index = 'i' values = 'data:post.labels' var = 'label'>
                                                            <li>
                                                                <a class='button button-small button-stable activated' expr:href='data:label.url'>
                                                                    <data:label.name/>
                                                                </a>
                                                            </li>
                                                        </b:loop>
                                                        </ul>
                                                    </h5>
                                                </nav>
                                            </b:if>

                                            <div class='post-social-share post_social_share text-center'></div>

                                            <b:if cond='data:blog.olderPageUrl or data:blog.newerPageUrl'>
                                                <table class='page-navi table-bordered table-hover'>
                                                    <tr>
                                                        <b:if cond='data:blog.olderPageUrl'>
                                                            <td>
                                                                <a class='older-link' expr:href='data:blog.olderPageUrl' rel='previous'>
                                                                    <span class='title'>
                                                                        <i class='fa fa-chevron-left'></i> Prev
                                                                    </span>
                                                                    <span class='subtitle'>
                                                                        <data:blog.olderPageTitle/>
                                                                    </span>
                                                                </a>
                                                            </td>
                                                        </b:if>
                                                        <b:if cond='data:blog.newerPageUrl'>
                                                            <td>
                                                                <a class='newer-link' expr:href='data:blog.newerPageUrl' rel='next'>
                                                                    <span class='title'>
                                                                        Next <i class='fa fa-chevron-right'></i>
                                                                    </span>
                                                                    <span class='subtitle'>
                                                                        <data:blog.newerPageTitle/>
                                                                    </span>
                                                                </a>
                                                            </td>
                                                        </b:if>
                                                    </tr>
                                                </table>
                                            </b:if>

                                            <!-- Related Post Widget Start -->
                                            <div id='related-posts' class='hidden-'>
                                                <h2 class='title'>Related Articles</h2>
                                                <div class='content'></div>
                                            </div>
                                        </b:if>
                                    </div>
                                    <div expr:id='&quot;footer-&quot; + data:post.id' class='panel-footer text-center'>
                                    </div>
                                    <!-- End post data -->
                                </article>
                            </b:loop>
                        </b:includable>
                    </b:widget>
                </b:section>
            </div>
            <!-- End of Main -->
            <!-- Start of Sidebar -->
            <div id='sidebar' class='col col-xs-12 col-sm-4'>
                <div class='post panel panel-default'>
                    <div class='panel-heading'>
                        <h3 class='title'>Search</h3>
                    </div>
                    <div class='panel-body'>
                        <form action="/search" method="get" role="search">
                            <label class="item item-input">
                                <i class="icon ion-search placeholder-icon"></i>
