    <body expr:class='data:blog.mobileClass'>
        
        <div id='page-type' class='param'><data:blog.pageType/></div>
        
        <!-- Start of SEO Meta Tag -->
        <b:if cond='data:blog.homepageUrl == data:blog.url'>
            <div class='meta-tag param'>
