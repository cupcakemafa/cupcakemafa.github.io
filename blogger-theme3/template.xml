    <head>
        <meta charset='utf-8' />
        <meta name='viewport' content='initial-scale=1, maximum-scale=1, user-scalable=no, width=device-width' />
        <meta content='IE=edge,chrome=1' http-equiv='X-UA-Compatible' />
        <!-- Start of all-head-content -->
        <b:include data='blog' name='all-head-content' />
        <!-- End of all-head-content -->
        <!-- SEO Meta Tag -->
        <b:if cond='data:blog.homepageUrl == data:blog.url'>
            <meta expr:content='data:blog.title' name='keywords' />
        </b:if>
        <b:if cond='data:blog.pageType == &quot;item&quot;'>
            <meta expr:content='data:blog.pageName' name='keywords' />
        </b:if>
        <b:if cond='data:blog.pageType == &quot;index&quot;'>
            <b:if cond='data:blog.searchLabel'>
                <meta content='noindex,nofollow' name='robots' />
            </b:if>
        </b:if>
        <b:if cond='data:blog.pageType == &quot;archive&quot;'>
            <meta content='noindex,nofollow' name='robots' />
        </b:if>
        <b:if cond='data:blog.isMobile'>
            <meta content='noindex,nofollow' name='robots' />
        </b:if>
        
        <title>
            <b:if cond='data:blog.url == data:blog.homepageUrl'>
            <b:elseif cond='data:blog.pageType == &quot;item&quot;'/>
                <data:blog.pageName/>
                 |
            <b:elseif cond='data:blog.pageType == &quot;archive&quot;'/>
                Archive for
                <data:blog.pageName/>
                 |
            <b:elseif cond='data:blog.pageType == &quot;static_page&quot;'/>
                <data:blog.pageName/>
                |
            <b:elseif cond='data:blog.pageType == &quot;index&quot;'/>
                <b:if cond='data:blog.searchLabel'>
                    <data:blog.title/> -
                    <data:blog.pageName/>
                    |
                <b:else/>
                    <b:if cond='data:blog.url != data:blog.homepageUrl'>
                        <data:blog.pageTitle/> - All Post | 
                    </b:if>
                </b:if>
            <b:elseif cond='data:blog.pageType == &quot;error_page&quot;'/>
                Page Not Found | 
            </b:if>
            <data:blog.title/>
        </title>
        <b:skin>
            <![CDATA[
            /*
            -----------------------------------------------
            Blogger Template Style
            Name:     Cupcakemafa Blogger Template
            Designer: Cupcakemafa
            URL:      http://www.cupcakemafa.com
