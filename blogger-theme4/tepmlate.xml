                    </b:if>
                </div>
                <!-- //.intro-text -->
            </div>
            <!-- //.container -->
        </header>
    </b:if>

    <b:section id='portfolio' class='bg-light-gray clearfix' showaddelement='no'>
        <b:widget id='Blog1' class='no-padding no-margin' locked='true' title='ブログの投稿' type='Blog' visible='true'>
            <b:includable id='main'>
                <div class='container'>
                    <b:if cond='data:blog.pageType in {&quot;index&quot;,&quot;archive&quot;}'>
                        <!-- List Page -->

                        <!-- Title for List Page -->
                        <div class='row'>
                            <div class='col-lg-12 text-center'>
                                <b:if cond='data:blog.homepageUrl == data:blog.url'>
                                    <!-- Top Page -->
                                    <h2 class='section-heading'>New Articles</h2>
                                    <p class='section-subheading text-muted'>
                                        ニュー・アーティコーたち<br />
                                        おもに旅とアートと本とケンのことについてです。
                                    </p>
                                    <!-- //Top Page -->
                                <b:elseif cond='data:blog.searchLabel != &quot;&quot;'/>
                                    <!-- Tag List -->
                                    <h2 class='section-heading'>
                                        <data:blog.searchLabel/>の記事リスト
                                    </h2>
                                    <!-- //Tag List -->
                                <b:elseif cond='data:blog.pageType in {&quot;archive&quot;}'/>
                                    <!-- Archive List -->
                                    <h2 class='section-heading'>
                                        <span class='title'></span> 記事リスト
                                    </h2>
                                    <!-- //Archive List -->
                                </b:if>
                            </div>
                        </div>
                        <!-- //Title for List Page -->

                        <!-- Post Content Area -->
                        <div class='row'>
