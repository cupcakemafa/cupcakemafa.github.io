        function setMetaTag() {
            var $metaTag = $('.meta-tag'),
                $elm, $attrs, $attr, key, value,
                metaTag = '',
                i, l,
                metaAttr;
            $metaTag.each(function (index, elm) {
                $elm = $(elm);
                metaAttr = '';
                $attrs = $elm.find('.attr');
                for (i = 0, l = $attrs.length; i < l; i++) {
                    $attr = $attrs.eq(i);
                    key = $attr.data('key');
                    value = $attr.text();
                    if (key && value) {
                        if (i > 0) {
                            metaAttr += ' ';
                        }
                        metaAttr += key + '="' + value + '"';
                    }
                    if (i === (l - 1)) {
                        metaTag += '<meta ' + metaAttr + '/>';
                    }
                }

                if (metaTag && index === ($metaTag.length - 1)) {
                    $('head').find('meta').last().after(metaTag);
                }
            });
        }
        setMetaTag();
        //]]>
    </script>
